{
  "name": "starter-ts",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "bun@1.2.6",
  "description": "_description_",
  "author": "Joydip Roy <rjoydip>",
  "license": "MIT",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint . --fix",
    "prepublishOnly": "bun run build",
    "release": "bumpp && pnpm publish",
    "start": "bun run src/index.ts",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@antfu/utils": "^9.1.0",
    "@types/node": "^22.13.11",
    "bumpp": "^10.1.0",
    "eslint": "^9.23.0",
    "lint-staged": "^15.5.0",
    "simple-git-hooks": "^2.12.1",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "vite": "^6.2.2",
    "vitest": "^3.0.9"
  },
  "simple-git-hooks": {
    "pre-commit": "bun i --ignore-scripts --offline && npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
